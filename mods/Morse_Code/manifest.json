{
    "manifest_version": 3,
    "name": "ChoccoBoard",
    "description": "keyboard sounds by me (ColdChocco)",
    "version": "1.0",
    "icons":
    {
        "512": "icon_512.png"
    },
    "developer":
    {
        "name": "ColdChocco"
    },
    "mod":
    {
        "schema_version": 1,
        "payload":
        {
            "keyboard_sounds":
            {
                "TYPING_LETTER":
                [
                    "sounds/Morse_Code_-_SHORT_update.mp3"
                ],
                "TYPING_ENTER":
                [
                    "sounds/Morse_Code_-_LONG_update.mp3"
                ],
                "TYPING_SPACE":
                [
                    "sounds/Morse_Code_-_LONG_update.mp3"
                ],
                "TYPING_BACKSPACE":
                [
                    "sounds/Morse_Code_-_LONG_update.mp3"
                ]
            }
        }
    }
}
